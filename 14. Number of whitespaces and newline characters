#include <iostream.h>
#include <string.h>

int main()
{
  int i;
  int w = 0;
  char a[] = {' ', '\n'};
  char x[30],z[30];

  for (int i = 0 ; i <= 30;i++)
    cin >> x[i];

  for (int j = 0 ; j < 30; j++) {
    for (int k = 0 ; k < 2; k++) {
      x[j] == a[k];
      if (x[j] == ' ')
        w++;
    }
  }

  cout << w << endl;
  system("pause");
  return  0;
}
